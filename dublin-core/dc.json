[

  {
    "label":"Contributor",
    "name":"contributor",
    "definition":"An entity responsible for making contributions to the resource.",
    "uri":"http://purl.org/dc/elements/1.1/contributor",
    "refinements":
      [
        "actor",
        "advisor","advisorChair","advisorDeptChair",
        "artist",
        "author",
        "designer",
        "director",
        "editor",
          "ArtEditor","CopyEditor","ExecutiveEditor","ManagingEditor","DeputyManagingEditor",
          "FeatureEditor","GraphicArtsEditor","NewsEditor","PhotographyEditor","SportsEditor",
          "CampusEditor","AsstPhotoEditor",
        "illustrator",

          "AdvManager","BusinessManager","ManagerOfPhotography",
        "organization",
        "other",
        "performer",
        "photographer",
        "producer",
        "reporter"
      ]
  },

  {
    "label": "Coverage",
    "name": "coverage",
    "definition": "The spatial or temporal topic of the resource, the spatial applicability of the resource, or the jurisdiction under which the resource is relevant.",
    "uri": "http://purl.org/dc/elements/1.1/coverage",
    "refinements":
      [
        "spatial", "temporal",

        "event", "other",
        "period", "periodName",
        "place","placeName"
      ]
  },

  {
    "label": "Creator",
    "name": "creator",
    "definition": "An entity primarily responsible for making the resource.",
    "uri": "http://purl.org/dc/elements/1.1/creator"
  },

  {
    "label": "Date",
    "name": "date",
    "definition": "A point or period of time associated with an event in the lifecycle of the resource.",
    "uri": "http://purl.org/dc/elements/1.1/date",
    "refinements":
      [
        "created","valid","available","issued","modified","dateaccepted","copyrighted","submitted",

        "accessioned","defense","digitized"
      ],
    "encoding": ["W3C-DTF"]
  },

  {
    "label": "Description",
    "name": "description",
    "definition": "An account of the resource.",
    "uri": "http://purl.org/dc/elements/1.1/description",
    "refinements":
      [
        "abstract", "tableofcontents",

        "approval", "audience", "author", "campus", "college", "defense",
        "department", "extent", "note", "other", "program", "provenance", "school",
        "sponsorship","statementOfResponsibility","uri"
      ]
  },

  {
    "label": "Format",
    "name": "format",
    "definition": "The file format, physical medium, or dimensions of the resource.",
    "uri": "http://purl.org/dc/elements/1.1/format",
    "refinements":
      [
        "extent","medium",

        "audio","carrierExtent","content","duration",
        "imageFormat","imageResolution",
        "mediaType","medium","mimetype","original","workFormat"
      ],
    "encoding": ["IMT"]
  },

  {
    "label": "Identifier",
    "name": "identifier",
    "definition": "An unambiguous reference to the resource within a given context.",
    "uri": "http://purl.org/dc/elements/1.1/identifier",
    "refinements": [ "bibliographiccitation","citation","govdoc","isbn","issn","other","uri","url" ],
    "encoding": ["URI"]
  },

  {
    "label": "Language",
    "name": "language",
    "definition": "A language of the resource.",
    "uri": "http://purl.org/dc/elements/1.1/language",
    "refinements": ["iso"],
    "encoding": ["ISO_639-2RFC_3066"]
  },

  {
    "label": "Publisher",
    "name": "publisher",
    "definition": "An entity responsible for making the resource available.",
    "uri": "http://purl.org/dc/elements/1.1/publisher",
    "refinements": ["namePublication"]
  },

  {
    "label": "Relation",
    "name": "relation",
    "definition": "A related resource.",
    "uri": "http://purl.org/dc/elements/1.1/relation",
    "refinements":
      [
        "isVersionOf","hasVersion",
        "isReplacedBy","replaces",
        "isRequiredBy","requires",
        "isReferencedBy","references",
        "isPartOf","hasPart","isPartOfSeries",
        "isFormatOf","hasFormat",
        "conformsTo",
        "uri"
      ]
  },

  {
    "label": "Rights",
    "name": "rights",
    "definition": "Information about rights held in and over the resource",
    "uri": "http://purl.org/dc/elements/1.1/rights",
    "refinements":
      [
        "accessrights","license",

        "availability","uri"
      ],
    "encoding":"URI"
  },

  {
    "label": "Source",
    "name": "source",
    "definition": "A related resource from which the described resource is derived.",
    "uri": "http://purl.org/dc/elements/1.1/source",
    "refinements": ["uri"],
    "encoding":"URI"
  },

  {
    "label": "Subject",
    "name": "subject",
    "definition": "The topic of the resource.",
    "uri": "http://purl.org/dc/elements/1.1/subject",
    "encoding":
      [
        "LCSH","MeSH","DDC","LCC","UDC",

        "ddc","keyword","lcc","lcnaf","lcsh","other"
      ]
  },

  {
    "label":"Title",
    "name":"title",
    "uri":"http://purl.org/dc/elements/1.1/title",
    "definition":"A name given to the resource.",
    "refinements":
      [
        "alternative",

        "article","series"
      ]
  },

  {
    "label":"Type",
    "name":"type",
    "uri":"http://purl.org/dc/elements/1.1/type",
    "definition":"The nature or genre of the resource.",
    "options":
      [
        "abstract","animation","article",
        "blog","book","bookChapter","bookReview",
        "capstone","caseStudy",
        "dataset","dissertation",
        "finalPaper",
        "image","image3D",
        "journal",
        "learningObject",
        "macromediaFlash","magazine","map","mastersProject","minutes","monograph","movie","musicalScore",
        "newsletter","newspaper",
        "plan","blueprint","podcast","policy","postprint","preprint","presentation","proceedings",
        "recording",
          "recordingAcoustic","recordingMusical","recordingOral",
        "report",
        "technicalReport","thesis",
        "video",
        "website","workingPaper"
      ],

    "encoding":[ "DCMIType" ]
  },



  {
    "label":"Audience",
    "name":"audience",
    "definition":"A class of entity for whom the resource is intended or useful.",
    "uri":"http://dublincore.org/documents/usageguide/elements.shtml#audience",
    "refinements":[ "educationLevel","mediator" ]
  },

  {
    "label": "Provenance",
    "name": "provenance",
    "definition": "A statement of any changes in ownership and custody of the resource since its creation that are significant for its authenticity, integrity and interpretation.",
    "uri": "http://dublincore.org/documents/usageguide/elements.shtml#provenance"
  },

  {
    "label": "RightsHolder",
    "name": "rightsHolder",
    "definition": "A person or organization owning or managing rights over the resource",
    "uri": "http://dublincore.org/documents/usageguide/elements.shtml#rightsHolder"
  }

]
